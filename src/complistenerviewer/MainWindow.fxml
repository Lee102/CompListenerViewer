<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="complistenerviewer.MainWindowController">
   <children>
      <TableView fx:id="workstationTableView" layoutX="14.0" layoutY="43.0" onMouseClicked="#workstationTableViewClick" prefHeight="200.0" prefWidth="365.0">
        <columns>
          <TableColumn fx:id="computerNameColumn" prefWidth="105.0" text="Computer name" />
            <TableColumn fx:id="userDomainColumn" prefWidth="85.0" text="User domain" />
            <TableColumn fx:id="userNameColumn" prefWidth="70.0" text="User name" />
            <TableColumn fx:id="macAddressColumn" prefWidth="90.0" text="MAC address" />
        </columns>
      </TableView>
      <TableView fx:id="wokstationAdditionalDataTableView" layoutX="636.0" layoutY="712.0" prefHeight="75.0" prefWidth="550.0">
        <columns>
          <TableColumn fx:id="osVersionColumn" prefWidth="70.0" text="OS version" />
          <TableColumn fx:id="osArchColumn" prefWidth="55.0" text="OS arch" />
            <TableColumn fx:id="javaVersionColumn" text="Java version" />
            <TableColumn fx:id="procIdColumn" prefWidth="55.0" text="CPU ID" />
            <TableColumn fx:id="procArchColumn" prefWidth="65.0" text="CPU arch" />
            <TableColumn fx:id="procCoresColumn" prefWidth="70.0" text="CPU cores" />
            <TableColumn fx:id="ramSizeColumn" prefWidth="70.0" text="RAM size" />
            <TableColumn fx:id="diskSizeColumn" prefWidth="70.0" text="Disk size" />
        </columns>
      </TableView>
      <TableView fx:id="windowTableView" layoutX="14.0" layoutY="251.0" onMouseClicked="#windowTableViewClick" prefHeight="215.0" prefWidth="365.0">
        <columns>
          <TableColumn fx:id="windowTitleColumn" prefWidth="190.0" text="Window title" />
          <TableColumn fx:id="startDateColumn" prefWidth="130.0" text="Start time" />
            <TableColumn fx:id="firstWindowColumn" prefWidth="30.0" text="First" />
        </columns>
      </TableView>
      <ImageView fx:id="printScreen" fitHeight="600.0" fitWidth="800.0" layoutX="386.0" layoutY="43.0" pickOnBounds="true" preserveRatio="true" />
      <TableView fx:id="keyboardClickTableView" layoutX="14.0" layoutY="474.0" prefHeight="170.0" prefWidth="215.0">
        <columns>
          <TableColumn fx:id="keyboardClickTimeColumn" prefWidth="130.0" text="Time" />
          <TableColumn fx:id="keyboardClickTextColumn" prefWidth="70.0" text="Key text" />
        </columns>
      </TableView>
      <TableView fx:id="mouseClickTableView" layoutX="14.0" layoutY="652.0" prefHeight="135.0" prefWidth="315.0">
        <columns>
          <TableColumn fx:id="mouseClickTimeColumn" prefWidth="130.0" text="Time" />
          <TableColumn fx:id="mouseClickButtonColumn" prefWidth="70.0" text="Button" />
            <TableColumn fx:id="mouseClickXColumn" prefWidth="50.0" text="X" />
            <TableColumn fx:id="mouseClickYColumn" prefWidth="50.0" text="Y" />
        </columns>
      </TableView>
      <TableView fx:id="mouseScrollTableView" layoutX="339.0" layoutY="652.0" prefHeight="135.0" prefWidth="215.0">
        <columns>
          <TableColumn fx:id="mouseScrollTimeColumn" prefWidth="130.0" text="Time" />
          <TableColumn fx:id="mouseScrollDirectionColumn" prefWidth="70.0" text="Direction" />
        </columns>
      </TableView>
      <MenuBar layoutY="2.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#saveDataAsMenuItemClick" text="Save data as..." />
                  <MenuItem mnemonicParsing="false" onAction="#savePictureAsMenuItemClick" text="Save picture as..." />
                  <MenuItem mnemonicParsing="false" onAction="#exitMenuItemClick" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
